{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest",
        "major"
      ],
      "automerge": true,
      "automergeType": "branch",
      "requiredStatusChecks": null
    }
  ],
  "platformAutomerge": true
}

